{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            "tab"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "object-literal-sort-keys": [
            false
        ],
        "one-variable-per-declaration": [
            false
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "no-angle-bracket-type-assertion": false,
        "trailing-comma": [
            false
        ],
        "no-shadowed-variable": false,
        "whitespace": [
            "check-typecast"
        ],
        "no-console": [
            false
        ],
        "no-string-literal": false,
        "ordered-imports": [
            false
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "variable-name": [
            "allow-leading-underscore"
        ],
        "max-classes-per-file": [
            false
        ],
        "array-type": [
            false
        ]
    },
    "rulesDirectory": []
}